<!DOCTYPE html>
<html lang="ja" class="no-js">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

<!-- HTML Meta Tags -->
<title>@ViewData["Title"]</title>
<meta name="description" content="Deep Insiderは、機械学習エンジニアを目指す人のための技術情報サイトとして2018年4月にオープン予定です！ 正式公開後、またお越しいただけますと幸いです。">

<!-- Google / Search Engine Tags -->
<meta itemprop="name" content="@ViewData["Title"]">
<meta itemprop="description" content="Deep Insiderは、機械学習エンジニアを目指す人のための技術情報サイトとして2018年4月にオープン予定です！ 正式公開後、またお越しいただけますと幸いです。">
<meta itemprop="image" content="https://deepinsider.jp/images/manabu-icon-with-logo-color.png">

<!-- Facebook Meta Tags -->
<meta property="og:url" content="https://deepinsider.jp">
<meta property="og:type" content="website">
<meta property="og:title" content="@ViewData["Title"]">
<meta property="og:description" content="Deep Insiderは、機械学習エンジニアを目指す人のための技術情報サイトとして2018年4月にオープン予定です！ 正式公開後、またお越しいただけますと幸いです。">
<meta property="og:image" content="https://deepinsider.jp/images/manabu-icon-with-logo-color.png">

<!-- Twitter Meta Tags -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="@ViewData["Title"]">
<meta name="twitter:description" content="Deep Insiderは、機械学習エンジニアを目指す人のための技術情報サイトとして2018年4月にオープン予定です！ 正式公開後、またお越しいただけますと幸いです。">
<meta name="twitter:image" content="https://deepinsider.jp/images/manabu-icon-with-logo-color.png">

<!-- Meta Tags Generated via http://heymeta.com -->


    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
        
    <environment include="Development">
        <link rel="stylesheet" href="~/css/style.css" />
        <link rel="stylesheet" href="~/css/normalize.css" />
        <link rel="stylesheet" href="~/css/style2.css" />
        <script type="text/javascript" src="~/js/jquery.particleground.js"></script>
        <script type="text/javascript" src="~/js/demo.js"></script>
        <script type="text/javascript" src="~/js/countdown.js"></script>
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
        <script type="text/javascript" src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

</head>
<body>
@RenderBody()
</body>
</html>
